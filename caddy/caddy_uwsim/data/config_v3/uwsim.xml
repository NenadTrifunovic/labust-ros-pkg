<launch>
    <node pkg="uwsim" name="uwsim" type="uwsim" 
  args="--configfile $(find caddy_uwsim)/data/scenes/test_environment.xml 
  --dataPath $(find caddy_uwsim)/data/scenes"  machine="shoreside"/>
    
    <node pkg="pladypos" type="NavSts2Odom.py" name="stateHat2uwsim" machine="shoreside" >
        <remap from="stateHat" to="pladypos/stateHat" />
        <remap from="uwsim_hook" to="pladypos/uwsim_hook" />
        <param name="tf_prefix" value="$(env TF_PREFIX)" if="$(env USE_TF_PREFIX)" />
    </node>
</launch>

<package>
	<description brief="UV applications.">
		The package contains the UV applications for running plugable vehicle driver. 
		It contains the related ROS nodes for vehicle control, identification and 
		navigation.
  </description>
  <author>Đula Nađ</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
	<!-- <url>http://ros.org/wiki/labust_uvapp</url> -->

	<depend package="roscpp" />
	<depend package="labust_tools" />
	<depend package="labust_xml" />
	<depend package="labust_navcon" />
	<depend package="labust_drivers" />
	<depend package="std_msgs" />
	<depend package="nav_msgs" />
	<depend package="MOOS_msgs" />
	
	<export>
	 <cpp cflags="-I${prefix}/include" lflags="-L${prefix}/lib -luvapp" />
	</export>

</package>



<package>
  <description brief="LABUST main code base.">
	The package performs a checkout of the labust-base repository. The repository
	is compiled and library exported to be used with ROS applications. The libraries
	and applications in labust-base are ROS agnostic and will remain so.
  </description>
  <author>Dula Nad</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <!-- <url>http://ros.org/wiki/labust_base</url> -->

  <rosdep name="boost" />
  <rosdep name="xml2" />
  <rosdep name="qt4" />
  <rosdep name="qwt" />

  <export>
	<cpp cflags="-I${prefix}/labust-base/src/labust_tools/include" />
	<cpp cflags="-I${prefix}/labust-base/src/labust_drivers/include" />
	<cpp cflags="-I${prefix}/labust-base/src/labust_xml/include" />
	<cpp cflags="-I${prefix}/labust-base/src/labust_uvsim/include" />
	<cpp cflags="-I${prefix}/labust-base/src/labust_navcon/include" />
	<cpp cflags="-I${prefix}/labust-base/src/labust_uvapp/include" />
	<cpp cflags="-I${prefix}/labust-base/src/reson_sdk/include" />
	<cpp cflags="-I${prefix}/labust-base/src/tritech_sdk/include" />
	<cpp cflags="-L${prefix}/labust-base/build/lib" />
  </export>

</package>



<launch>

    <!-- ========================================================= -->
    <!-- == Arguments ============================================ -->
    <!-- ========================================================= -->

    <!--<arg name="labust_mission_path" />
    <arg name="xml_save_path" />
    <arg name="mission_execution" default="true" />
    <arg name="neptus_parser" default="true" />
    <arg name="parseGUI" default="true" />-->


    <!-- ========================================================= -->
    <!-- == CourseKeeping FA node ================================ -->
    <!-- ========================================================= -->

    <node 
        pkg="labust_primitives" 
        type="course_keeping_FA"
        name="course_keeping_FA" >

     	<remap from="out" to="stateRef" />
     	<remap from="state" to="stateHat" />
	<param name="underactuated" value="false" />

    </node>

    <!-- ========================================================= -->
    <!-- == CourseKeeping UA node ================================ -->
    <!-- ========================================================= -->

    <node 
        pkg="labust_primitives" 
        type="course_keeping_UA"
        name="course_keeping_UA" >

     	<remap from="out" to="stateRef" />
     	<remap from="state" to="stateHat" />
	<param name="underactuated" value="true" />

    </node>

    <!-- ========================================================= -->
    <!-- == Dynamic Positioning node ============================= -->
    <!-- ========================================================= -->

    <node 
        pkg="labust_primitives"
        type="dp_primitive"
        name="dp_primitive">
     	
        <remap from="out" to="stateRef" />
     	<remap from="state" to="stateHat" />
        <param name="underactuated" value="false" />

     </node>

    <!-- ========================================================= -->
    <!-- == Go2Point FA node ===================================== -->
    <!-- ========================================================= -->

    <node 
        pkg="labust_primitives"
        type="go2point_FA"
        name="go2point_FA">

     	<remap from="out" to="stateRef" />
     	<remap from="state" to="stateHat" />
        <param name="underactuated" value="false" />

    </node>

    <!-- ========================================================= -->
    <!-- == Go2Point UA node ===================================== -->
    <!-- ========================================================= -->

    <node 
        pkg="labust_primitives"
        type="go2point_UA"
        name="go2point_UA">

     	<remap from="out" to="stateRef" />
     	<remap from="state" to="stateHat" />
        <param name="underactuated" value="true" />

    </node>
   
</launch>

<launch>
  <include file="$(find caddy_uwsim)/data/config/$(env ROBOT).machine" /> 
    
  <include file="$(find caddy_uwsim)/data/config/uwsim.xml" />
  
  <group ns="pladypos">
  	<rosparam command="load" file="$(find caddy_uwsim)/data/config/$(env LOCATION).yaml" />  	
  	<include file="$(find caddy_uwsim)/data/config/navigation.xml" />
  	<include file="$(find caddy_uwsim)/data/config/control.xml" />
   	<include file="$(find caddy_uwsim)/data/config/joy.xml" />
  	<include file="$(find caddy_uwsim)/data/config/driver.xml" />     
  </group>
</launch>

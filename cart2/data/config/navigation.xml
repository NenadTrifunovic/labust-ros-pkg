<launch>
  <rosparam command="load" file="$(env YAML_MODEL)" />
  <node pkg="pladypos" type="nav_node" name="ekf_nav" machine="controlled">
      <param name="tf_prefix" value="$(env TF_PREFIX)" if="$(env USE_TF_PREFIX)" />
  </node>
  
  <node pkg="cart2" type="imu_node" name="imu" machine="controlled" if="$(env USE_CART)" >
    <param name="PortName" value="/dev/ttyS1" />
    <param name="Baud" value="115200" />
    <param name="gps_pub" value="10" />
    <remap from="fix" to="gps" />
    <param name="tf_prefix" value="$(env TF_PREFIX)" if="$(env USE_TF_PREFIX)" />
    <param name="magnetic_declination" value="0.122172944" />
  </node> 
</launch>

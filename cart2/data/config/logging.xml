<launch> 
    <rosparam command="load" file="$(find cart2)/data/config/logger.yaml" />
    <node pkg="cart2" name="logger" type="matlogger2.py">
      <param name="dir" value="$(env LOGDIR)" />
    </node>
    
    <node pkg="rosbag" type="rosbag" name="rosbag"
       args="record -o $(env LOGDIR)/rosmsgs" />
</launch>